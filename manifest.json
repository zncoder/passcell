{
  "name": "PassCell",
  "version": "1.8.8",
  "manifest_version": 2,
  "description": "Generate and manage passwords",
  "icons": {
    "48": "icons/48.png"
  },
	"permissions": [
		"storage",
		"activeTab",
		"<all_urls>",
		"contextMenus"
	],
  "background": {
    "scripts": [ "common.js", "background.js" ]
  },
	"browser_action": {
		"browser_style": true,
		"default_popup": "popup.html",
		"default_icon": "icons/32.png",
		"default_title": "Manage password of this site"
	}
}
